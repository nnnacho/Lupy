<header>
  <section class="font-poppins dark:bg-gray-800">
    <div class="max-w-6xl px-4 mx-auto" x-data="{open:false}">
      <nav class="flex items-center justify-between py-4">
        <a
          href=""
          class="text-3xl font-semibold leading-none dark:text-gray-400"
          ><svg
            xmlns="http://www.w3.org/2000/svg"
            class="icon icon-tabler icon-tabler-dog"
            width="50"
            height="50"
            viewBox="0 0 24 24"
            stroke-width="1.5"
            stroke="currentColor"
            fill="none"
            stroke-linecap="round"
            stroke-linejoin="round"
            ><path stroke="none" d="M0 0h24v24H0z" fill="none"></path><path
              d="M11 5h2"></path><path
              d="M19 12c-.667 5.333 -2.333 8 -5 8h-4c-2.667 0 -4.333 -2.667 -5 -8"
            ></path><path d="M11 16c0 .667 .333 1 1 1s1 -.333 1 -1h-2z"
            ></path><path d="M12 18v2"></path><path d="M10 11v.01"></path><path
              d="M14 11v.01"></path><path
              d="M5 4l6 .97l-6.238 6.688a1.021 1.021 0 0 1 -1.41 .111a.953 .953 0 0 1 -.327 -.954l1.975 -6.815z"
            ></path><path
              d="M19 4l-6 .97l6.238 6.688c.358 .408 .989 .458 1.41 .111a.953 .953 0 0 0 .327 -.954l-1.975 -6.815z"
            ></path></svg
          ></a
        >
        <div class="flex justify-between lg:space-x-9">
          <div class="lg:hidden">
            <button
              class="flex items-center px-3 py-2 text-orange-600 border border-orange-200 rounded dark:text-gray-400 navbar-burger hover:text-orange-800 hover:border-orange-300 lg:hidden"
              @click="open =true"
            >
              <svg
                xmlns="http://www.w3.org/2000/svg"
                width="18"
                height="18"
                fill="currentColor"
                class="bi bi-list"
                viewBox="0 0 16 16"
              >
                <path
                  fill-rule="evenodd"
                  d="M2.5 12a.5.5 0 0 1 .5-.5h10a.5.5 0 0 1 0 1H3a.5.5 0 0 1-.5-.5zm0-4a.5.5 0 0 1 .5-.5h10a.5.5 0 0 1 0 1H3a.5.5 0 0 1-.5-.5zm0-4a.5.5 0 0 1 .5-.5h10a.5.5 0 0 1 0 1H3a.5.5 0 0 1-.5-.5z"
                ></path>
              </svg>
            </button>
          </div>
          <ul class="hidden lg:w-auto lg:space-x-9 lg:items-center lg:flex">
            <li>
              <a
                href="#"
                class="text-sm text-gray-700 hover:text-orange-700 dark:text-gray-400 dark:hover:text-orange-500"
                >Inicio</a
              >
            </li>
            <li>
              <a
                href="#servicios"
                class="text-sm text-gray-700 hover:text-orange-700 dark:text-gray-400 dark:hover:text-orange-500"
                >Servicios</a
              >
            </li>
            <li>
              <a
                href="#precios"
                class="text-sm text-gray-700 hover:text-orange-700 dark:text-gray-400 dark:hover:text-orange-500"
                >Precios</a
              >
            </li>
            <li>
              <a
                href="#contacto"
                class="text-sm text-gray-700 hover:text-orange-700 dark:text-gray-400 dark:hover:text-orange-500"
                >Contacto
              </a>
            </li>
          </ul>
          <div class="hidden lg:block">
            <a
              href=""
              class="inline-block px-4 py-3 mr-2 text-xs font-medium leading-none text-gray-100 bg-[#ff8e3c] rounded-full dark:hover:bg-orange-500 dark:hover:text-gray-200 dark:text-gray-900 dark:bg-orange-400 hover:bg-orange-700"
            >
              Contactanos
            </a>
          </div>
        </div>
      </nav>
      <!-- Mobile Sidebar -->
      <div
        class="fixed inset-0 w-full bg-gray-900 opacity-25 dark:bg-gray-400 lg:hidden"
        :class="{'translate-x-0 ease-in-opacity-100' :open===true, '-translate-x-full ease-out opacity-0' : open===false}"
      >
      </div>
      <div
        class="absolute inset-0 z-10 h-screen p-3 text-gray-400 duration-500 transform bg-orange-50 dark:bg-gray-800 w-80 lg:hidden lg:transform-none lg:relative"
        :class="{'translate-x-0 ease-in-opacity-100' :open===true, '-translate-x-full ease-out opacity-0' : open===false}"
      >
        <div class="flex justify-between lg:">
          <a
            class="p-2 text-2xl font-bold text-gray-700 dark:text-gray-400"
            href="#">Logo</a
          >
          <button
            class="p-2 text-gray-700 rounded-md dark:text-gray-400 hover:text-orange-300 lg:hidden"
            @click="open=false"
          >
            <svg
              xmlns="http://www.w3.org/2000/svg"
              width="24"
              height="24"
              fill="currentColor"
              class="bi bi-x-circle"
              viewBox="0 0 16 16"
            >
              <path
                d="M8 15A7 7 0 1 1 8 1a7 7 0 0 1 0 14zm0 1A8 8 0 1 0 8 0a8 8 0 0 0 0 16z"
              ></path>
              <path
                d="M4.646 4.646a.5.5 0 0 1 .708 0L8 7.293l2.646-2.647a.5.5 0 0 1 .708.708L8.707 8l2.647 2.646a.5.5 0 0 1-.708.708L8 8.707l-2.646 2.647a.5.5 0 0 1-.708-.708L7.293 8 4.646 5.354a.5.5 0 0 1 0-.708z"
              ></path>
            </svg>
          </button>
        </div>
        <ul class="px-4 text-left mt-7">
          <li class="pb-3">
            <a
              href=""
              class="text-sm text-gray-700 hover:text-orange-400 dark:text-gray-100"
              >Home</a
            >
          </li>
          <li class="pb-3">
            <a
              href=""
              class="text-sm text-gray-700 hover:text-orange-400 dark:text-gray-400"
              >About us</a
            >
          </li>
          <li class="pb-3">
            <a
              href=""
              class="text-sm text-gray-700 hover:text-orange-400 dark:text-gray-400"
              >Features</a
            >
          </li>
          <li class="pb-3">
            <a
              href=""
              class="text-sm text-gray-700 hover:text-orange-400 dark:text-gray-400"
              >Blog
            </a>
          </li>
          <li class="pb-3">
            <a
              href=""
              class="text-sm text-gray-700 hover:text-orange-400 dark:text-gray-400"
              >Testimonials</a
            >
          </li>
        </ul>
        <div class="block mt-5 lg:hidden">
          <a
            href=""
            class="inline-block w-full px-4 py-3 mr-2 text-xs font-semibold leading-none text-center text-gray-100 bg-orange-600 rounded-full dark:hover:bg-orange-400 dark:bg-orange-300 dark:text-gray-700 hover:bg-orange-700"
          >
            Contact us
          </a>
        </div>
      </div>
    </div>
  </section>
  <script defer src="https://unpkg.com/alpinejs@3.x.x/dist/cdn.min.js"></script>
</header>
